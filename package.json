{
  "name": "template-rollup-ts",
  "version": "1.0.0",
  "description": "A Phaser 3 TypeScript template using Rollup.",
  "scripts": {
    "dev": "rollup --watch --config rollup/rollup.config.dev.mjs --no-treeshake",
    "build": "rollup --config rollup/rollup.config.prod.mjs",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "start": "npx serve dist -s -l 4000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phaserjs/template-rollup-ts.git"
  },
  "author": "Phaser Studio <support@phaser.io> (https://phaser.io/)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/phaserjs/template-rollup-ts/issues"
  },
  "homepage": "https://github.com/phaserjs/template-rollup-ts#readme",
  "dependencies": {
    "phaser": "^3.70.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.8",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "javascript-obfuscator": "^4.1.0",
    "rollup": "^4.9.5",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-glslify": "^1.3.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-obfuscator": "^1.1.0",
    "rollup-plugin-serve": "^1.1.1",
    "serve": "^14.2.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
